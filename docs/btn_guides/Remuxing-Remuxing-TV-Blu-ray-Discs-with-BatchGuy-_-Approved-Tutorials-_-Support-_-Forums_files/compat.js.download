$(document).ready(function(){(function setPlaceholderText(){if(!Modernizr.placeholder){$('[placeholder]').each(function(){this.value=this.placeholder;}).focus(function(){if(this.value==this.placeholder){this.value='';}}).blur(function(){if(this.value==''){this.value=this.placeholder;}});}}());(function setAutoFocusForm(){if(!Modernizr.input.autofocus){$('[autofocus]').focus();}}());});